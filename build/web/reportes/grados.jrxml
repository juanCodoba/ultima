	<queryString>
		<![CDATA[select reserva_libros_biblioteca.id_grado as idGrado,
grado.grado as nombreGrado,
proyectocfip.reserva_libros_biblioteca.id_usuario_prestamo as idUsuarioPrestamo,
count(proyectocfip.reserva_libros_biblioteca.id_grado) as total
