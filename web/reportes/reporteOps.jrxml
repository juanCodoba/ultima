	<queryString>
		<![CDATA[SELECT
sum(planilla.cantidad) as total_prueba_cantidad,
sum(planilla.valor_subtotal) as total_prueba,
sum(planilla.cantidad - planilla.confeccion) as total_cantidad_confeccion,
 planilla.pv_planilla, planilla.id_item_planilla,planilla.talla,
planilla.descripcion_planilla,planilla.modulo_prod

from proyectocfip.planilla
inner join  proyectocfip.op_planilla on planilla.id_op_planilla = op_planilla.id_op_planilla
where proyectocfip.op_planilla.op_planilla =  $P{opPlanilla}
group by  op_planilla.op_planilla, planilla.pv_planilla, planilla.id_item_planilla,
planilla.descripcion_planilla,planilla.modulo_prod;]]>
	</queryString>
